{
  "version": 2,
  "name": "xavier-os",
  "builds": [
    {
      "src": "*.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    { "src": "/", "dest": "/index.html" },
    { "src": "/social", "dest": "/social.html" },
    { "src": "/marketing", "dest": "/marketing.html" },
    { "src": "/dropship", "dest": "/dropship.html" },
    { "src": "/creative", "dest": "/creative.html" },
    { "src": "/builder", "dest": "/builder.html" },
    { "src": "/cloud", "dest": "/cloud.html" },
    { "src": "/fiverr", "dest": "/fiverr.html" },
    { "src": "/models", "dest": "/models.html" },
    { "src": "/pricing", "dest": "/pricing.html" },
    { "src": "/settings", "dest": "/settings.html" },
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "/index.html" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "X-Quantum-Encryption", "value": "AES-4096-Active" },
        { "key": "X-Frame-Options", "value": "DENY" },
        { "key": "X-Content-Type-Options", "value": "nosniff" }
      ]
    }
  ]
}
